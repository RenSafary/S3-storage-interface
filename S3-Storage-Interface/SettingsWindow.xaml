<Window x:Class="S3_Storage_Interface.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:S3_Storage_Interface"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="200" Width="270">
    <Grid>
        <Label Content="Endpoint URL" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Bucket Name" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label Content="Access Key" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Width="128"/>
        <Label Content="Secret Access Key" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Endpoint_URL" HorizontalAlignment="Left" Margin="101,14,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Bucket_Name" HorizontalAlignment="Left" Margin="101,45,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Access_Key_Id" HorizontalAlignment="Left" Margin="101,76,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Secret_Access_Key" HorizontalAlignment="Left" Margin="123,107,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Apply" HorizontalAlignment="Left" Margin="113,142,0,0" VerticalAlignment="Top" Click="Button_Click" RenderTransformOrigin="0.351,-1.272"/>


    </Grid>
</Window>
